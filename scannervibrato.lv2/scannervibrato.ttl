@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#> .
@prefix epp:   <http://lv2plug.in/ns/ext/port-props#> .
@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<https://github.com/jkbd#me>
       a foaf:Person ;
       foaf:name "Jakob DÃ¼bel" ;
       foaf:mbox <20183922+jkbd@users.noreply.github.com> ;
       foaf:homepage <https://github.com/jkbd> .         

<https://github.com/jkbd/scannervibrato>
       a lv2:Plugin ,
         lv2:ModulatorPlugin ;
       a doap:Project ;
       doap:name "Scannervibrato" ;
       lv2:binary <scannervibrato.so> ;
       lv2:optionalFeature epp:supportsStrictBounds ;
       lv2:optionalFeature lv2:hardRTCapable ;
       doap:maintainer <https://github.com/jkbd#me> ;
       a doap:Version ;
       doap:revision "1.0.0" ;
       doap:license "MIT" ;
    lv2:port [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 0 ;
	lv2:symbol "AM_0" ;
	lv2:name "Amplitude Modulation" ,
		 "Amplitudenmodulation"@de ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 150 ;
	lv2:default 0 ;
	lv2:minimum 0 ;
	lv2:maximum 0.15 ;
	units:unit units:coef ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 1 ;
	lv2:symbol "Depth_1" ;
	lv2:name "Depth" ,
		 "Tiefe"@de ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:integer ;	
        epp:rangeSteps 2 ;
	lv2:default 1 ;
	lv2:minimum 1 ;
	lv2:maximum 3 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 2 ;
	lv2:symbol "Mode_2" ;
	lv2:name "Mode" ,
		 "Modus"@de ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:integer ;
        epp:rangeSteps 2 ;
	lv2:default 0 ;
	lv2:minimum 0 ;
	lv2:maximum 2 ;
	lv2:scalePoint [
	  rdfs:label "Vibrato" ;
	  rdf:value 2
	] , [
	  rdfs:label "Chorus" ;
	  rdf:value 1
	] , [
	  rdfs:label "Bypass" ;
	  rdf:value 0
	]
    ] , [
	a lv2:InputPort ;
	a lv2:AudioPort ;
	lv2:index 3 ;
	lv2:symbol "in0" ;
	lv2:name "in0" ;
    ] , [
	a lv2:OutputPort ;
	a lv2:AudioPort ;
	lv2:index 4 ;
	lv2:symbol "out0" ;
	lv2:name "out0" ;
    ]
.
